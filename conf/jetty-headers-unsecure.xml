<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
  <entry key="Content-Security-Policy">default-src http:; script-src 'unsafe-inline' 'self'; style-src 'unsafe-inline' 'self'; font-src data: 'self'; img-src data: 'self'</entry>
  <entry key="X-Content-Type-Options">nosniff</entry>
  <entry key="X-XSS-Protection">1; mode=block</entry>
  <entry key="Strict-Transport-Security">max-age=31536000;includeSubDomains</entry>
</properties>
